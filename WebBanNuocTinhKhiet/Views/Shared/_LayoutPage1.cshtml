                                        <script>
                                            function toggleSearchBlock1() {
                                                var searchBlock = document.querySelector('.search-block');
                                                var bodyOverlay = document.getElementById('body_overlay');

                                                // Toggle 'open' class on searchBlock
                                                searchBlock.classList.toggle('open');

                                                // Toggle 'd-none' class on bodyOverlay
                                                bodyOverlay.classList.toggle('d-none');

                                                // Add mousedown event listener to the document body
                                                document.body.addEventListener('mousedown', handleBodyClick);
                                            }

                                            function handleBodyClick(event) {
                                                var searchBlock = document.querySelector('.search-block');
                                                var bodyOverlay = document.getElementById('body_overlay');

                                                // Check if the clicked element is not within the searchBlock or bodyOverlay
                                                if (!searchBlock.contains(event.target)) {
                                                    // Remove 'open' class from searchBlock and add 'd-none' class to bodyOverlay
                                                    searchBlock.classList.remove('open');
                                                    bodyOverlay.classList.add('d-none');

                                                    // Remove the mousedown event listener from the document body
                                                    document.body.removeEventListener('mousedown', handleBodyClick);
                                                }
                                            }
                                        </script>


